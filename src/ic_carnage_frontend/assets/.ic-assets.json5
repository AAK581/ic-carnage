[
  {
    "match": "index.html",
    "headers": {
      "Content-Security-Policy": "default-src 'self' data:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://unpkg.com blob:; script-src-elem 'self' 'unsafe-inline' 'unsafe-eval' blob: https://unpkg.com; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' https://ic0.app http://localhost:4943 http://127.0.0.1:4943 https://icp-api.io ws://localhost:4943 ws://127.0.0.1:4943 https://unpkg.com; worker-src 'self' blob:;"
    }
  },
  {
    "match": "**/*.unityweb",
    "headers": {
      "Content-Encoding": "br",
      "Content-Type": "application/octet-stream"
    }
  },
  {
    "match": "**/*.loader.js",
    "headers": {
      "Content-Type": "application/javascript"
    }
  },
  {
    "match": "**/*.framework.js.unityweb",
    "headers": {
      "Content-Encoding": "br",
      "Content-Type": "application/javascript"
    }
  },
  {
    "match": "**/*.wasm.unityweb", 
    "headers": {
      "Content-Encoding": "br",
      "Content-Type": "application/wasm"
    }
  },
  {
    "match": "**/*.data.unityweb",
    "headers": {
      "Content-Encoding": "br",
      "Content-Type": "application/octet-stream"
    }
  },
  {
    "match": "**/*.js",
    "headers": {
      "Content-Type": "application/javascript"
    }
  },
  {
    "match": "**/*",
    "security_policy": "standard"
  }
]